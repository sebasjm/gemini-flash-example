{
  "name": "merchanthub",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:css": "tailwindcss -i ./src/main.css -o ./dist/styles.css",
    "build:js": "node esbuild.config.js",
    "build:html": "cp src/index.html dist/index.html",
    "build": "npm run build:css && npm run build:js && npm run build:html",
    "release": "pnpm run build && sudo rm -rf ./docs/ && mv dist/ ./docs/",
    "dev": "npm run build && esbuild src/index.tsx --bundle --outfile=dist/index.js --servedir=dist --serve=8000"
  },
  "dependencies": {
    "preact": "^10.22.0",
    "lucide-preact": "^0.363.0",
    "@google/genai": "^1.39.0",
    "i18next": "^23.10.0"
  },
  "devDependencies": {
    "esbuild": "^0.20.2",
    "tailwindcss": "^3.4.1",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35"
  },
  "packageManager": "pnpm@9.7.0+sha512.dc09430156b427f5ecfc79888899e1c39d2d690f004be70e05230b72cb173d96839587545d09429b55ac3c429c801b4dc3c0e002f653830a420fa2dd4e3cf9cf"
}
